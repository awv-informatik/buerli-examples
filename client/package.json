{
  "name": "buerli-examples-client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run copy && vite",
    "build": "tsc && npm run copy && vite build",
    "serve": "npm run copy && vite preview",
    "lint": "eslint \"./src/**/*.ts?(x)\"",
    "prettify": "prettier --write \"./{src,tests}/**/*.{js,jsx,ts,tsx,json}\"",
    "copy": "copyfiles -a -u 1 \"./src/models/**/*.*\" \"./public\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@buerli.io/headless": "0.10.0",
    "@buerli.io/react": "0.10.0",
    "@codemirror/lang-javascript": "^6.0.2",
    "@mdx-js/react": "2.1.0",
    "@react-three/drei": "9.89.0",
    "@react-three/fiber": "8.13.0",
    "@uiw/react-codemirror": "^4.11.4",
    "antd": "^4.23.4",
    "arraybuffer-loader": "1.0.8",
    "buffer": "^6.0.3",
    "immer": "9.0.5",
    "path-browserify": "^1.0.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-router-dom": "5.2.0",
    "styled-components": "5.3.1",
    "three": "0.153"
  },
  "devDependencies": {
    "@types/path-browserify": "^1.0.0",
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0",
    "@types/styled-components": "^5.1.14",
    "@types/three": "0.152.1",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitejs/plugin-react": "^4.0.4",
    "babel-plugin-import": "1.13.3",
    "copyfiles": "^2.4.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "7.0.2",
    "less": "4.1.2",
    "lint-staged": "11.1.2",
    "prettier": "3.0.1",
    "rimraf": "3.0.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-dynamic-import": "^1.5.0",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
